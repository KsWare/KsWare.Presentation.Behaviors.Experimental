<UserControl 
    x:Class="KsWare.Presentation.Behaviors.Experimental.TestApp.IsFocusedPropertyBehaviorTest"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:ksv="http://ksware.de/Presentation/ViewFramework"
             xmlns:testApp="clr-namespace:KsWare.Presentation.Behaviors.Experimental.TestApp"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <UserControl.DataContext>
        <testApp:IsFocusedPropertyBehaviorTestViewModel />
    </UserControl.DataContext> 
    <behaviors:Interaction.Behaviors>
        <ksv:IsFocusedPropertyBehavior IsElementFocused="{Binding IsFocused}"/>
    </behaviors:Interaction.Behaviors>
    <Grid>
            
    </Grid>
</UserControl>
